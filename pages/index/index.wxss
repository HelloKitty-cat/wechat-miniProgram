/* pages/index/index.wxss */
page {
  height: 100%;
  background: #8ed145;
}
.indexContainer {
  display: flex;
  flex-direction: column;
  align-items: center
}

.indexImage {
  width: 250rpx;
  height: 250rpx;
  border-radius: 50%;
  margin: 100rpx;
}

.indexText {
  font-size: 40rpx;
  font-weight: bold;
  margin: 100rpx;
}

.indexGo {
  width: 280rpx;
  height: 80rpx;
  border: 1rpx solid #fff;
  font: 24rpx/80rpx "微软雅黑";
  text-align: center;
  border-radius: 8rpx;
  color: white;
  margin: 100rpx;
}
.indexBtn {
  width: 250rpx;
  height: 250rpx;
  border-radius: 50%;
  font-size: 24rpx;
  line-height: 250rpx;
  margin: 100rpx;
  background: #FFFAFA;
}