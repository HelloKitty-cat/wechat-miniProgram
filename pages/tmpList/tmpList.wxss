/* pages/tmpList/tmpList.wxss */
.tmpContainer {
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  margin: 6rpx 0;
}
.tmpBigImage {
  width: 100%;
  height: 400rpx;
}
.collection_share image {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.collection_share text {
  font-size: 24rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}